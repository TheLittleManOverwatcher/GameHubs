local playerhead = game.Players.LocalPlayer.Character.Torso

for i, v in pairs(game:GetService("Workspace").Weapons["AK-47"].Hitbox:GetDescendants()) do
    if v.Name == "TouchInterest" and v.Parent then
        firetouchinterest(playerhead, v.Parent, 0)
        wait(0.1)
        firetouchinterest(playerhead, v.Parent, 1)
    end
end

wait()

